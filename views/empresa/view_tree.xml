<odoo>
    <!-- Vista de Lista para Empresa -->
    <record model="ir.ui.view" id="view_empresa_tree">
        <field name="name">empresa.ver.tree</field>
        <field name="model">instituto.empresa</field>
        <field name="arch" type="xml">
            <tree>
                        <field name="nombre"/>
                        <field name="direccion"/>
                        <field name="telefono"/>
                        <field name="departamento"/>
                        <field name="alumnado"/>
            </tree>
        </field>
    </record>
</odoo>